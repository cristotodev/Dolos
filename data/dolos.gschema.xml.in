<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="@APP_NAME@">
	<schema id="@APP_ID@" path="@APP_ROOT@/">
		<key name="window-width" type="i">
			<default>1000</default>
		</key>
		<key name="window-height" type="i">
			<default>700</default>
		</key>
		<key name="body-wrap" type="b">
			<default>true</default>
			<summary>Wrap the body content</summary>
		</key>
		<key name="show-line-numbers" type="b">
			<default>false</default>
			<summary>Show line numbers in response JSON</summary>
		</key>
		<key name="indent-content" type="b">
			<default>true</default>
			<summary>Indent content in response JSON</summary>
		</key>
	</schema>
</schemalist>
